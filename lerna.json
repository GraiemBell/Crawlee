{
  "packages": [
    "packages/*"
  ],
  "version": "3.3.2",
  "command": {
    "version": {
      "conventionalCommits": true,
      "createRelease": "github",
      "message": "chore(release): %s"
    }
  },
  "npmClient": "yarn",
  "useWorkspaces": true,
  "useNx": false,
  "ignoreChanges": [
    "**/test/**",
    "**/*.md"
  ]
}
