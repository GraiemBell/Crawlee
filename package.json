{
    "name": "apify",
    "version": "0.20.0",
    "description": "The scalable web crawling and scraping library for JavaScript/Node.js. Enables development of data extraction and web automation jobs (not only) with headless Chrome and Puppeteer.",
    "engines": {
        "node": ">=10.17.0"
    },
    "main": "build/index.js",
    "types": "types/index.esm.d.ts",
    "keywords": [
        "apify",
        "headless",
        "chrome",
        "puppeteer",
        "crawler",
        "scraper"
    ],
    "author": {
        "name": "Apify",
        "email": "support@apify.com",
        "url": "https://apify.com"
    },
    "contributors": [
        "Jan Curn <jan@apify.com>",
        "Marek Trunkat <marek@apify.com>",
        "Ondra Urban <ondra@apify.com>"
    ],
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/apifytech/apify-js"
    },
    "bugs": {
        "url": "https://github.com/apifytech/apify-js/issues"
    },
    "homepage": "https://sdk.apify.com/",
    "files": [
        "build",
        "types"
    ],
    "scripts": {
        "build": "rm -rf ./build && babel src --out-dir build --copy-files",
        "build-docs": "npm run clean && npm run build && node ./website/tools/build_docs.js && npm run build-types",
        "build-readme": "node ./tools/build_readme.js",
        "build-types": "./node_modules/.bin/tsc -p tsconfig.json && node ./tools/typescript_fixes.js",
        "test": "npm run build && npm run build-types && jest --testTimeout=20000 --maxWorkers=3",
        "prepare": "npm run build",
        "prepublishOnly": "(test $TRAVIS || (echo \"Publishing is reserved to the Travis CI!\"; exit 1))",
        "clean": "rm -rf build",
        "lint": "npm run build && ./node_modules/.bin/eslint ./src ./test",
        "lint:fix": "./node_modules/.bin/eslint ./src ./test --ext .js,.jsx --fix"
    },
    "dependencies": {
        "@apify/http-request": "^1.1.5",
        "@apify/ps-tree": "^1.1.3",
        "@types/cheerio": "^0.22.15",
        "@types/node": "^13.1.8",
        "@types/puppeteer": "^2.0.0",
        "@types/tough-cookie": "^2.3.6",
        "apify-client": "^0.5.25",
        "apify-shared": "^0.1.66",
        "cheerio": "^1.0.0-rc.3",
        "content-type": "^1.0.4",
        "express": "^4.17.1",
        "fs-extra": "^8.1.0",
        "htmlparser2": "^3.10.1",
        "iconv-lite": "^0.5.0",
        "jquery": "^3.4.1",
        "mime-types": "^2.1.24",
        "proxy-chain": "^0.3.3",
        "request": "^2.88.0",
        "request-promise-native": "^1.0.5",
        "rimraf": "^2.7.1",
        "semver": "^6.3.0",
        "socket.io": "^2.2.0",
        "tough-cookie": "^3.0.1",
        "ts-jest": "^24.3.0",
        "underscore": "^1.9.1",
        "ws": "^6.2.1"
    },
    "devDependencies": {
        "@apify/eslint-config": "^0.0.3",
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/register": "^7.0.0",
        "@types/fs-extra": "^7.0.0",
        "@types/jest": "^24.0.21",
        "@types/sinon": "^7.0.13",
        "@types/socket.io": "^2.1.2",
        "basic-auth-parser": "^0.0.2",
        "body-parser": "^1.19.0",
        "csv-parse": "^4.4.3",
        "eslint": "^5.16.0",
        "fpcollect": "^1.0.4",
        "fpscanner": "^0.1.5",
        "jest": "^24.9.0",
        "jsdoc": "^3.5.5",
        "jsdoc-export-default-interop": "^0.3.1",
        "portastic": "^1.0.1",
        "prettier": "^1.18.2",
        "proxy": "^1.0.1",
        "sinon": "^7.4.1",
        "sinon-stub-promise": "^4.0.0",
        "socket.io-client": "^2.2.0",
        "typescript": "^3.7.4",
        "ua-parser-js": "^0.7.20",
        "why-is-node-running": "^2.0.3"
    },
    "optionalDependencies": {
        "puppeteer": "2.1.1"
    }
}
